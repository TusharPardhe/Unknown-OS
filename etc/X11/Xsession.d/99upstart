# Start the user session
if [ -n "$UPSTART" ]; then
    case "$BASESESSION" in
        gnome-session*|gnome-flashback*)
            export SESSIONTYPE=gnome-session
            ;;
        lxsession*)
            export SESSIONTYPE=lxsession
            ;;
    esac

    STARTUP="/sbin/upstart --user"
    unset UPSTART
fi
