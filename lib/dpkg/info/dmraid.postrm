#!/bin/sh

set -e

case "$1" in
    remove|purge)
        if command -v update-initramfs >/dev/null 2>&1; then
		update-initramfs -u
    	fi

     ;;
    upgrade|failed-upgrade|abort-install|abort-upgrade|disappear)


        ;;

    *)
        echo "postrm called with unknown argument \`$1'" >&2
        exit 1

esac





