#!/bin/sh
set -e

case "$1" in
    remove|deconfigure)
	update-alternatives --remove rmt /usr/sbin/rmt-tar
	if test -L /sbin/rmt && test /sbin/rmt -ef /usr/sbin/rmt; then
		rm -f /sbin/rmt
	fi
    ;;
    upgrade|failed-upgrade)
    ;;
    *)
	echo "prerm called with unknown argument \`$1'" >&2
	exit 1
    ;;
esac


